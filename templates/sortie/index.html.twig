{% extends 'base.html.twig' %}

{% block title %}Hello SortieController!{% endblock %}

{% block body %}
<h1>Liste des Sorties</h1>

<table >
    {% for sortie in sorties %}
    <tr>
        {#                <td>{{ wish.dateCreated|date('d/m/y H:i') }}</td>#}
        <td><a href="{{ path('sortie_detail',{'id':sortie.id })}}">{{ sortie.nom }}</a></td>
        <td {{ sortie.dateHeureDebut |date('Y-m-d H:i:s')  }} {{ sortie.dateLimiteInscription |date('Y-m-d H:i:s')  }} ></td>

    </tr>


    {% endfor %}
</table>
{% endblock %}
