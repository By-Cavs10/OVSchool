

{% block title %}{% endblock %}

{% block body %}

    <h1>Detail de la Sortie {{ sortie.id }}</h1>

    <table>

        <tr>

            <li>{{ sortie.nom}}</li>
            <li>{{ sortie.infosSortie }}</li>


            {% for site in sortie.site %}
                <li>{{ site.nom }}</li>
            {% endfor %}

            <h2>Partipants : </h2>
            {% for user in sortie.participants %}
                <li>{{ user.nom }}</li>
                <li>{{ user.prenom }}</li>
            {% endfor %}




        </tr>


    </table>

